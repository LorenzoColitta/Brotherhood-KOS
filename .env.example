# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here
DISCORD_GUILD_ID=your_guild_id_for_guild_commands_optional

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_ANON_KEY=your_anon_key_here

# API Security
API_SECRET_KEY=your_api_secret_key_here

# Admin Password (Optional - for automated setup)
# If set, the set-admin-password script will use this instead of prompting
ADMIN_PASSWORD=your_admin_password_here

# Telegram Configuration (Optional - for notifications)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_optional
TELEGRAM_CHAT_ID=your_telegram_chat_id_optional

# Environment
NODE_ENV=production
